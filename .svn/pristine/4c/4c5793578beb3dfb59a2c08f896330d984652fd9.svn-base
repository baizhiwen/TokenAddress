package org.ktrade.address.demo.util;

public class StringUtil {
    public static String toLength(String str, int length) {
        if (str == null) {
            return null;
        }
        StringBuffer sb=new StringBuffer();
        int x =  length - str.length() ;

        for (int i=0;i<x;i++){
            sb.append(" ");
        }
        sb.append(str);
        return sb.toString();
    }
}
